# إعدادات مسارات البيانات
CHATGPT_EXPORT_PATH=path/to/conversations.json
HISTORY_OUTPUT_DIR=history/
NOTES_DIR=notes/

# إعدادات معالجة النصوص
MAX_FILE_SIZE=10485760
ENCODING=utf-8
LANGUAGE=ar

# إعدادات البحث
SEARCH_THRESHOLD=0.7
MAX_RESULTS=10
ENABLE_FUZZY_SEARCH=true

# إعدادات النماذج المحلية (اختيارية)
LOCAL_MODEL_PATH=models/
MODEL_TYPE=llama

# إعدادات الضغط والأرشفة
COMPRESSION_LEVEL=6
BACKUP_DIR=backups/